<ResourceDictionary x:Class="VoiceCraft.Client.Themes.DarkPurple.Colors" xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Dark">
            <!--  Accents  -->
            <Color x:Key="AccentColor">#1751C3</Color>
            <Color x:Key="ErrorAccentColor">#E0A030</Color>
            <Color x:Key="SuccessAccentColor">#3A823D</Color>

            <!--  Backgrounds  -->
            <Color x:Key="BackgroundColor">#88562971</Color>
            <Color x:Key="BackgroundTagColor">#88666666</Color>
            <Color x:Key="ApplicationBackgroundColor">#646</Color>
            <!--  We have to do this because foregrounds are broken on the avalonia notification styling.  -->
            <Color x:Key="NotificationBackgroundColor">#444</Color>
            <Color x:Key="ErrorBackgroundColor">#800</Color>
            <Color x:Key="SuccessBackgroundColor">#0A0</Color>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>